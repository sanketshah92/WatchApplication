<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="@dimen/border_stroke_width"
                android:color="@color/dimmed_menu_item_bg_color" />
            <corners android:radius="@dimen/border_radius" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/space"
        android:left="@dimen/space"
        android:right="@dimen/space"
        android:top="@dimen/space">
        <shape android:shape="rectangle">
            <solid android:color="@color/dimmed_menu_item_bg_color" />
            <corners android:radius="@dimen/border_radius" />
        </shape>
    </item>
</layer-list>
